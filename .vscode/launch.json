{
    "version": "0.1.0-alpha",
    "configurations": [
		{
			"type": "firefox",
			"request": "launch",
			"reAttach": true,
			"name": "Launch extension",
			"addonPath": "${workspaceFolder}/dist",
			"preLaunchTask": "npm: build:dev",
			"pathMappings": [
				{
					"url": "webpack://sketchy/src",
					"path": "${workspaceFolder}/src"
				}
			]
		}
	]
}
