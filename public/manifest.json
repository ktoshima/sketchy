{
	"manifest_version": 3,
	"name": "Sketchy",
	"version": "0.1",
  "description": "Slideshow using Google Images",
	"homepage_url": "https://github.com/ktoshima/sketchy",
	"action": {
		"default_popup": "index.html",
		"default_title": "Open Sketchy"
	},
  "host_permissions": [
    "https://*.google.com/search?*"
  ],
  "permissions": [
    "scripting",
    "activeTab"
  ],
	"background": {
		"scripts": [],
		"persistent": false,
		"type": "module"
	}
}
